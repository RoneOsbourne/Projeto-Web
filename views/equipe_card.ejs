<div class="card mb-3 equipe">
    <div class="row no-gutters">
      <div class="col-md-4">
        <img src="/images/<%= equipe.foto %>" class="card-img" >
      </div>
      <div class="col-md-8">
        <div class="card-body">
          <h5 class="card-title"><%= equipe.nome %></h5>
          <p class="card-text"><%= equipe.cargo %></p>
          <p class="card-text"><small class="text-muted"><a href="<%= equipe.linkedin %>">Linkedin</a></small></p>
        </div>
      </div>
    </div>
  </div>